{
  "name": "@crsx/linkify",
  "version": "1.0.2",
  "description": "A lightweight utility that detects and converts plain text URLs into clickable HTML links",
  "author": "Cristian Militaru",
  "license": "ISC",
  "type": "module",
  "main": "./dist/fesm2020/crsx-linkify.mjs",
  "module": "./dist/fesm2015/crsx-linkify.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "postbuild": "node tools/fix-dist-package.cjs"
  },
  "keywords": [
    "link",
    "url",
    "pipe",
    "html",
    "text"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=14"
  },
  "peerDependencies": {
    "@angular/common": ">=14",
    "@angular/core": ">=14"
  },
  "devDependencies": {
    "@angular/compiler": "^14.3.0",
    "@angular/compiler-cli": "^14.3.0",
    "ng-packagr": "^14.2.2",
    "tslib": "^2.8.1",
    "typescript": "~4.7.4"
  }
}